<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MainServlet</servlet-name>
  	<display-name>MainServlet</display-name>
  	<description></description>
  	<servlet-class>com.trsis.MainServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreateDepartureServlet</servlet-name>
  	<display-name>CreateDepartureServlet</display-name>
  	<description></description>
  	<servlet-class>com.trsis.CreateDepartureServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteDepartureServlet</servlet-name>
  	<display-name>DeleteDepartureServlet</display-name>
  	<description></description>
  	<servlet-class>com.trsis.DeleteDepartureServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MainServlet</servlet-name>
  	<url-pattern>/MainServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreateDepartureServlet</servlet-name>
  	<url-pattern>/createDeparture</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteDepartureServlet</servlet-name>
  	<url-pattern>/deleteDeparture</url-pattern>
  </servlet-mapping>
</web-app>
